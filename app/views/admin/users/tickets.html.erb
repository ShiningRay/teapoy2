<table>
  <thead>
    <tr>
      <th>文章编号</th>
      <th>你的投票</th>
      <th>结果</th>
      <th>时间</th>
    </tr>
  </thead>
<% @tickets.each do |t| %>
  <tr>
    <td>#<%= t.article_id %></td>
    <% if t.ticket_type %>
    <td><%= t.ticket_type.name  %></td>
    <td><%= t.correct ? '正确' : '错误' unless t.correct.nil?%></td>
    <td><%=t.created_at.strftime("%Y-%m-%d %H:%M:%S")%></td>
    <td><%= t.created_at - t.viewed_at if t.viewed_at%> s </td>
    <% else %>
    <td colspan="2">跳过</td>
    <% end %>
  </tr>
<% end %>
</table>
<%=will_paginate @tickets%>